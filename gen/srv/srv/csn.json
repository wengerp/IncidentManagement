        "code": {
          "key": true,
          "type": "cds.String",
          "enum": {
            "new": {
              "val": "N"
            },
            "assigned": {
              "val": "A"
            },
            "in_process": {
              "val": "I"
            },
            "on_hold": {
              "val": "H"
